{
  "name": "infra",
  "version": "1.0.0",
  "description": "webpack boilerplate",
  "module": "./dist/infra.js",
  "main": "./dist/infra.js",
  "files": [
    "./dist/",
    "./src/"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "start": "webpack-dev-server --mode development --open",
    "prebuild": "yarn run clean",

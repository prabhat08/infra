    "dev": "webpack --watch --mode development --env expose",
    "build-lib": "webpack --mode production --env expose",
    "build-lib-analyze": "webpack --mode production --env expose --env analyze"
  },
  "author": "Prabhat Kumar",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.8",
